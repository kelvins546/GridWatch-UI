{
  "expo": {
    "name": "GridWatch",
    "slug": "GridWatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gridwatch_icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gridwatch_icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.gridwatch.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_STATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b47693f5-a608-4eb6-bf85-58ffa2a0ef67"
      }
    }
  }
}
